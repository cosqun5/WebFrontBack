@model TeamMember;
<div class="col-md-6 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">

            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="Id"></label>
                    <input type="hidden" asp-for="Id" class="form-control" placeholder="Name">

                </div>
                <div class="form-group">
                    <label asp-for="FulName"></label>
                    <input asp-for="FulName" class="form-control" placeholder="Category">
                    <span class="text text-danger" asp-validation-for="FulName"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Profection"></label>
                    <input asp-for="Profection" class="form-control" placeholder="Category">
                    <span class="text text-danger" asp-validation-for="Profection"></span>
                </div>
             @*   <div class="form-group">
                    <label asp-for="Photo"></label>
                    <input asp-for="Photo" class="form-control" placeholder="Category">
                    <span class="text text-danger" asp-validation-for="Photo"></span>
                </div>*@
                <button type="submit" class="btn btn-primary me-2 text-light">Edit</button>
                <button class="btn btn-light" asp-action="Index">Go back</button>
            </form>
        </div>
    </div>
</div>

